{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "data-integration": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/data-integration",
      "sourceRoot": "projects/data-integration/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "modules/data-integration",
            "baseHref": "./",
            "browser": "projects/data-integration/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/data-integration/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/data-integration/public"
              }
            ],
            "styles": [
              "projects/data-integration/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "data-integration:build:production"
            },
            "development": {
              "buildTarget": "data-integration:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/data-integration/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/data-integration/public"
              }
            ],
            "styles": [
              "projects/data-integration/src/styles.css"
            ]
          }
        }
      }
    },
    "code-development": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/code-development",
      "sourceRoot": "projects/code-development/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "modules/code-development",
            "baseHref": "./",
            "browser": "projects/code-development/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/code-development/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/code-development/public"
              }
            ],
            "styles": [
              "projects/code-development/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "code-development:build:production"
            },
            "development": {
              "buildTarget": "code-development:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/code-development/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/code-development/public"
              }
            ],
            "styles": [
              "projects/code-development/src/styles.css"
            ]
          }
        }
      }
    }
  }
}
