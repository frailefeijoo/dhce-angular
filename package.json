{
  "name": ".",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:data-integration": "ng serve data-integration",
    "start:code-development": "ng serve code-development",
    "build": "npm run build:data-integration && npm run build:code-development && npm run modules:manifest",
    "build:data-integration": "ng build data-integration && node scripts/promote-index.mjs modules/data-integration",
    "build:code-development": "ng build code-development && node scripts/promote-index.mjs modules/code-development",
    "build:code-devolpment": "npm run build:code-development",
    "modules:manifest": "node scripts/generate-modules-manifest.mjs",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.16",
    "@angular/cli": "^20.3.1",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}